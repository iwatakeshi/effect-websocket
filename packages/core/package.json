    "monorepo",
    "node",
    "bun",
    "browser",
    "rpc",
    "functional-programming"
  ],
  "homepage": "https://github.com/iwatakeshi/effect-websocket#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/iwatakeshi/effect-websocket.git"
  },
